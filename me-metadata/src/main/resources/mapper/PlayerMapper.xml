<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.me.metadata.db.mapper.PlayerMapper">

    <select id="getAll" resultType="com.me.metadata.db.entity.PlayerEntity">
        select *
        from t_player
    </select>

    <select id="getAllCount" resultType="int">
        SELECT COUNT(*)
        FROM t_player;
    </select>
</mapper>